{
  "name": "@endpass/connect-demo",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "SOURCE_MAP=true PORT=4000 vue-cli-service serve ./src/main.js",
    "dev:e2e": "vue-cli-service serve ./src/main.js --mode e2e-connect",
    "build": "rimraf ./dist && vue-cli-service build ./src/main.js",
    "build:e2e": "rimraf ./dist && vue-cli-service build ./src/main.js --mode e2e-connect",
    "format:prettier:vue": "prettier --write '**/*.vue'",
    "format:eslint": "eslint --ext .json,.js,.vue,.ts src/ --fix",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:prettier:vue && yarn format:eslint | exit 0 && yarn format:prettier",
    "check-types": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,json}": [
      "vue-cli-service lint --fix --quiet",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "vue-cli-service lint --fix --quiet",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@endpass/class": "^0.17.3",
    "@endpass/connect": "^0.27.3-beta",
    "@endpass/e2e-utils": "^0.6.0",
    "@endpass/ui": "^0.15.44",
    "@endpass/utils": "^1.8.2",
    "bulma": "^0.7.5",
    "vue": "^2.6.10",
    "vue-notification": "^1.3.16",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-class-modules": "^1.1.0",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@endpass/eslint-plugin-endpass": "^0.2.4",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-e2e-cypress": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.2.0",
    "svgo": "^1.2.2",
    "svgo-loader": "^2.2.1",
    "typescript": "^3.5.3",
    "utility-types": "^3.7.0",
    "vue-cli-plugin-svg": "^0.1.3",
    "vue-cli-plugin-svg-sprite": "^1.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
